{
  "manifest_version": 2,

  "name": "WebUiSys",
  "description": "This extension allows the user to change the background color of the current page.",
  "version": "1.0",
  "background": {
    "persistent": false,
    "scripts":[
      "js/jquery.min.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "wus_popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus",
    "notifications",
    "<all_urls>",
    "tabs",
    "downloads",
    "downloads.open"
  ]
}
